{
    "name": "drnkwati/org-jsonapi-php",
    "description": "The PHP implementation of JSON API document.",
    "license": "MIT",
    "type": "library",
    "authors": [
    {
        "name": "Divine Nkwati ",
        "email": "drnkwati@gmail.com"
    }],
    "require":
    {
        "php": ">=7.0"
    },
    "require-dev":
    {
        "symfony/var-dumper": ">=5.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload":
    {
        "psr-4":
        {
            "Org\\Jsonapi\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Tests\\": "tests/"
        }
    },
    "extra":
    {
        "branch-alias":
        {
            "dev-master": "1.0-dev"
        }
    },
    "config":
    {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}